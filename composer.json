{
  "name": "backtend/phplib",
  "description": "Several commonly used php libraries",
  "license": "Apache-2.0",
  "type": "library",
  "authors": [
    {
      "name": "backtend",
      "email": "backtend@gmail.com"
    }
  ],
  "keywords": [
    "RedisLib",
    "QrcodeLib",
    "MarkdownLib",
    "CrawlLib",
    "TreeLib"
  ],
  "require": {
